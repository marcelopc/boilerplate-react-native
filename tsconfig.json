{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*", "node_modules/*", "src/types/*"]
    },
    "jsx": "react-jsx", // Habilita suporte a JSX,
    "typeRoots": ["./node_modules/@types", "./src/types"],
  },
  "exclude": ["node_modules"],
   "include": ["src/**/*", "__tests__/**/*"],
}
